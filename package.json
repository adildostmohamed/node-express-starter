{
  "name": "node-express-starter",
  "version": "1.0.0",
  "description": "A basic starter kit for a node and express app with gulp and browser-sync and support for ES6",
  "main": "server.js",
  "scripts": {
    "dev": "gulp js && gulp prod && concurrently \"nodemon --exec babel-node src/server.js\" \"gulp watch\"",
    "prestart": "babel src --out-dir dist && gulp js && gulp prod",
    "start": "node dist/server.js"
  },
  "author": "Adil Dostmohamed",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.1",
    "bootstrap-sass": "^3.3.6",
    "browser-sync": "^2.12.10",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-order": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglifyjs": "^0.6.2",
    "jquery": "^2.2.3",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.16",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "nodemon": "^1.11.0",
    "browser-sync": "^2.12.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-order": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglifyjs": "^0.6.2"
  }
}
